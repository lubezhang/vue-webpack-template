{
  "name": "vue-webpack-template",
  "version": "0.1.0",
  "main": "index.js",
  "author": "lubezhang@gmail.com",
  "license": "MIT",
  "scripts": {
    "build": "node ./build/prod.js",
    "dev": "node ./build/dev.js",
    "test:e2e": "karma start test/e2e/karma.conf.js --single-run",
    "test:unit": "jest ./test/unit/*.test.js",
    "test:unit:cover": "npm run test:unit  -- --coverage",
    "test": "export BABEL_ENV=test && npm run test:e2e && npm run test:unit:cover"
  },
  "dependencies": {
    "vue": "^2.4.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "chalk": "^2.1.0",
    "chromedriver": "^2.32.3",
    "css-loader": "^0.28.7",
    "eslint": "^4.7.2",
    "jest": "^21.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^1.3.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "mocha": "^3.5.3",
    "opn": "^5.1.0",
    "ora": "^1.3.0",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.13.0",
    "vue-jest": "^1.0.0",
    "vue-loader": "^13.0.5",
    "vue-server-renderer": "2.4.4",
    "vue-template-compiler": "^2.4.4",
    "vue-test-utils": "^1.0.0-beta.4",
    "webpack": "2.x",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "mapCoverage": true,
    "coverageDirectory": "<rootDir>/test/unit/coverage"
  }
}
